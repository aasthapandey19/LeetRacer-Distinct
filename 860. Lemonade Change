class Solution {
public:
    bool lemonadeChange(vector<int>& bills) {
        if(bills[0]==20) return false;
        int five=0,ten=0;
        int temp=0;
        for(auto x:bills){
            if(x==5) five++;
            else if(x==10) five--,ten++;
            else if(ten>0)ten--,five--;
            else five-=3;
            if(five<0) return false;
        }
        return true;
    }
};
